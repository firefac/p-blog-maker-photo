page{
  background-color: #fff;
}

.container {
  box-sizing: border-box;
  background-color: #fff;
  font-family: PingFangSC-Light, helvetica, 'Heiti SC';
}

.noContent{
  display: block;
  line-height: 80rpx;
  text-align: center;
  margin-top: 100rpx;
  font-size: 30rpx;
  color: #666;
}

.title {
  font-size: 30rpx;
  font-weight: bold;
  display: block;
  color: #000;
  padding: 15rpx;
  height: 110rpx;
  overflow:hidden;
}

.img{
  width: 100%;
  height: 365rpx;
  padding: 10rpx;
  border: 1rpx solid #999;
}
